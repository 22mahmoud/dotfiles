{
  "rust-analyzer.serverPath": "/usr/bin/rust-analyzer",
  "rust-analyzer.checkOnSave.command": "clippy",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte"
  ],
  "prettier.disableSuccessMessage": true,
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "javascript",
    "graphql",
    "html",
    "rust",
    "json",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "lua",
    "svelte",
    "ejs"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "explorer.width": 30,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
    "v": "open:vsplit"
  },
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    "**/src/**"
  ],
  "cSpell.userWords": [
    "Alacritty",
    "Ashraf",
    "CLIs",
    "Gmail",
    "Mahmoud",
    "Ripgrep",
    "Tldr",
    "allacrtiy",
    "cheatsheets",
    "degoogle",
    "foss",
    "mahmoudashraf",
    "nord",
    "unalias"
  ],

  "tailwindCSS.emmetCompletions": true
}
